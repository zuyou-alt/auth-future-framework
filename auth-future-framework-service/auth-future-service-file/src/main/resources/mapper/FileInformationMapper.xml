<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="auth.future.service.file.mapper.FileInformationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="auth.future.service.file.entity.FileInformation">
        <id column="F_ID" property="id" />
        <result column="F_CREATE_BY" property="createBy" />
        <result column="F_CREATE_TIME" property="createTime" />
        <result column="F_UPDATE_BY" property="updateBy" />
        <result column="F_UPDATE_TIME" property="updateTime" />
        <result column="F_FILE_NAME" property="fileName" />
        <result column="F_ORIGINAL_FILE_NAME" property="originalFileName" />
        <result column="F_FILE_SIZE" property="fileSize" />
        <result column="F_PATH" property="path" />
        <result column="F_BASE_PATH" property="basePath" />
        <result column="F_EXT" property="ext" />
        <result column="F_FILE_PLATFORM" property="filePlatform" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        F_CREATE_BY,
        F_CREATE_TIME,
        F_UPDATE_BY,
        F_UPDATE_TIME,
        F_ID, F_FILE_NAME, F_ORIGINAL_FILE_NAME, F_FILE_SIZE, F_PATH, F_BASE_PATH, F_EXT, F_FILE_PLATFORM
    </sql>

</mapper>
